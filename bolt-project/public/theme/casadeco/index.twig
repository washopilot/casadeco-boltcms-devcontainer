{% extends 'partials/_master.twig' %}

{% block main %}

    {% for section in record.sections_home %}

        {% if section.name == 'hero_section' %}
            {% include 'partials/_hero.twig' with { content: section } %}
        {% endif %}

        {% include 'partials/_content.twig' with { section: section } %}

    {% endfor %}


{% endblock main %}
